<?xml version="1.0"?>
<plugin label="accessories/plugins/NodeNote">
	<plugin_registration class_name="accessories.plugins.NodeNote$Registration" isPluginBase="true">
		<plugin_mode class_name="freemind.modes.mindmapmode"/>
	</plugin_registration>
	<!--fc, 23.7.06: notes are now integrated.
		<plugin_action documentation="%accessories/plugins/NodeNote.properties_documentation" name="%accessories/plugins/NodeNote.properties_name" key_stroke="%keystroke_accessories/plugins/NodeNote.properties_key" base="freemind.extensions.NodeHookAdapter" icon_path="images/knotes.png" label="accessories/plugins/NodeNote.properties" class_name="accessories.plugins.NodeNote" instanciation="Once">
		<plugin_mode class_name="freemind.modes.mindmapmode"/>
		<plugin_menu location="menu_bar/insert/nodeNote"/>
	</plugin_action>-->
	<plugin_action documentation="%accessories/plugins/NodeNote_jumpto.properties_documentation" 
		name="%accessories/plugins/NodeNote_jumpto.properties_name" 
		key_stroke="%keystroke_accessories/plugins/NodeNote_jumpto.keystroke.alt_N" 
		base="freemind.extensions.NodeHookAdapter" 
		label="accessories/plugins/NodeNote_jumpto.properties" 
		class_name="accessories.plugins.NodeNote" 
		instanciation="Other">
        <plugin_mode class_name="freemind.modes.mindmapmode"/>
        <plugin_menu location="menu_bar/navigate/notes/note_jumpto"/>
        <plugin_property name="direction" value="note"/>
    </plugin_action>
	<plugin_action documentation="%accessories/plugins/NodeNote_jumpfrom.properties_documentation" 
		name="%accessories/plugins/NodeNote_jumpfrom.properties_name" 
		key_stroke="%keystroke_accessories/plugins/NodeNote_jumpfrom.keystroke.alt_M" 
		base="freemind.extensions.NodeHookAdapter" 
		label="accessories/plugins/NodeNote_jumpfrom.properties" 
		class_name="accessories.plugins.NodeNote" 
		instanciation="Other">
        <plugin_mode class_name="freemind.modes.mindmapmode"/>
        <plugin_menu location="menu_bar/navigate/notes/note_jumpfrom"/>
        <plugin_property name="direction" value="node"/>
    </plugin_action>

</plugin>
